<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 128 128">
  <!-- Background -->
  <rect width="128" height="128" fill="#667eea" rx="24"/>
  
  <!-- Video play symbol with sweep -->
  <g transform="translate(64, 64)">
    <!-- Play triangle -->
    <polygon points="-20,-25 -20,25 20,0" fill="white" opacity="0.9"/>
    
    <!-- Sweep arc -->
    <path d="M 0,-35 A 35,35 0 0,1 35,0" stroke="white" stroke-width="4" fill="none" stroke-linecap="round" opacity="0.7"/>
    
    <!-- Center dot -->
    <circle cx="0" cy="0" r="3" fill="white"/>
  </g>
</svg>
